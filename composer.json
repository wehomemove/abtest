{
    "name": "wehomemove/abtest",
    "description": "A comprehensive Laravel A/B testing package with user-organized event tracking, interactive dashboard, and count-based analytics",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Homemove",
            "email": "support@homemove.com"
        }
    ],
    "keywords": [
        "laravel",
        "ab-testing",
        "split-testing",
        "conversion-optimization",
        "analytics",
        "dashboard",
        "experimentation"
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Homemove\\AbTesting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Homemove\\AbTesting\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Homemove\\AbTesting\\Providers\\AbTestingServiceProvider"
            ],
            "aliases": {
                "AbTest": "Homemove\\AbTesting\\Facades\\AbTest"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}